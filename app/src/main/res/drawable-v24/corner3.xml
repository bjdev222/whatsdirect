<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <solid android:color="#dcf8c6"></solid>

    <!-- Assign border color and width of border -->
    <stroke
        android:width="3dp"
        android:color="#ffffff"></stroke>

    <!-- To add some padding -->
    <padding
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp"></padding>

    <!-- Assign corner radius here -->
    <corners android:radius="10dp"></corners>
</shape>